{
  "siteMetadata": {
    "language": "en",
    "siteUrl": "https://gatsby-starter-portfolio-minimal-theme.netlify.app/",
    "title": "NBA All-World Database",
    "titleTemplate": "%s",
    "description": "A database fan site of the NBA All-World mobile game.",
    "author": "Drew Wei",
    "avatar": "../content/images/nba-all-world.png",
    "bio": "",
    "social": {
      "github": "https://github.com/unkdrew/nba-all-world-db"
    }
  },
  "siteConfiguration": {
    "logo": { "text": "NBA All-World Database" },
    "navigation": {
      "header": [
        { "label": "About", "url": "/#about" },
        { "label": "Players", "url": "/players" }
      ],
      "footer": [
        { "label": "Privacy", "url": "/privacy" },
        { "label": "Imprint", "url": "/imprint" }
      ]
    },
    "featureToggles": {
      "useDarkModeAsDefault": false,
      "useDarkModeBasedOnUsersPreference": true,
      "useCookieBar": false
    }
  }
}
